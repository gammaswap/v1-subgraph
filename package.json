{
  "name": "gammaswap-subgraph",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "mustache networks/arbitrum-goerli.json subgraph.template.yaml subgraph.arbitrum-goerli.yaml",
    "prepare:tc": "mustache networks/arbitrum-goerli.tc.json subgraph.tc.template.yaml subgraph.tc.arbitrum-goerli.yaml",
    "codegen": "graph codegen subgraph.arbitrum-goerli.yaml --output-dir src/types/",
    "codegen:tc": "graph codegen subgraph.tc.arbitrum-goerli.yaml --output-dir src/types/tc",
    "build": "graph build subgraph.arbitrum-goerli.yaml",
    "build:tc": "graph build subgraph.tc.arbitrum-goerli.yaml",
    "deploy": "graph deploy --product hosted-service kalesberg/gammaswap-subgraph subgraph.arbitrum-goerli.yaml",
    "deploy:tc": "graph deploy --product hosted-service kalesberg/gammaswap-trading-competition subgraph.tc.arbitrum-goerli.yaml"
  },
