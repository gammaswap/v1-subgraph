{
  "name": "gammaswap-subgraph",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare:arbgoerli": "mustache networks/arbitrum-goerli.json subgraph.template.yaml subgraph.arbitrum-goerli.yaml && mustache networks/arbitrum-goerli.json src/helpers/constants.template.ts src/helpers/constants.ts",
    "prepare:arbsepolia": "mustache networks/arbitrum-sepolia.json subgraph.template.yaml subgraph.arbitrum-sepolia.yaml && mustache networks/arbitrum-sepolia.json src/helpers/constants.template.ts src/helpers/constants.ts",
    "prepare:arbitrum": "mustache networks/arbitrum.json subgraph.template.yaml subgraph.arbitrum.yaml && mustache networks/arbitrum.json src/helpers/constants.template.ts src/helpers/constants.ts",
    "prepare:tc": "mustache networks/arbitrum-goerli.tc.json subgraph.tc.template.yaml subgraph.tc.arbitrum-goerli.yaml",
    "codegen": "graph codegen subgraph.arbitrum-goerli.yaml --output-dir src/types/",
    "codegen:tc": "graph codegen subgraph.tc.arbitrum-goerli.yaml --output-dir src/types/tc",
    "build": "graph build subgraph.arbitrum-goerli.yaml",
    "build:arbsepolia": "graph build subgraph.arbitrum-sepolia.yaml",
    "build:arbitrum": "graph build subgraph.arbitrum.yaml",
    "build:tc": "graph build subgraph.tc.arbitrum-goerli.yaml",
    "deploy:tc": "graph deploy --product hosted-service kalesberg/gammaswap-trading-competition subgraph.tc.arbitrum-goerli.yaml",
    "deploy": "graph deploy --product hosted-service gammaswap/gammaswap subgraph.arbitrum.yaml",
    "deploy:dev": "graph deploy --product hosted-service gammaswap/gammaswap-dev subgraph.arbitrum-goerli.yaml",
    "deploy:arbgoerli": "graph deploy --product hosted-service gammaswap/gammaswap-v1_1-arbgoerli subgraph.arbitrum-goerli.yaml",
    "deploy:arbgoerli-dev": "graph deploy --product hosted-service gammaswap/gammaswap-v1_1-arbgoerli-dev subgraph.arbitrum-goerli.yaml",
    "deploy:arbsepolia": "graph deploy --product hosted-service gammaswap/gammaswap-v1-arbsepolia subgraph.arbitrum-sepolia.yaml",
    "deploy:arbsepolia-dev": "graph deploy --product hosted-service gammaswap/gammaswap-v1-arbsepolia-dev subgraph.arbitrum-sepolia.yaml",
    "deploy:arbitrum": "graph deploy --product hosted-service gammaswap/gammaswap-v1-arbitrum subgraph.arbitrum.yaml"
  },
  "keywords": [],
  "author": "Simon Mall",
  "license": "MIT",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.51.2",
    "@graphprotocol/graph-ts": "^0.31.0",
    "mustache": "^4.2.0"
  }
}
